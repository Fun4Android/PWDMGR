{
  "manifest_version": 2,
  "name": "PWDMGR",
  "version": "3.0",
  "description": "Password Manager",
  "homepage_url": "https://github.com/dosisod/PWDMGR",
  "icons": {
    "32": "img/32.png",
    "48": "img/48.png"
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "img/32.png",
    "default_title": "PWDMGR",
    "default_popup": "src/index.html"
  }
}
